{
  "name": "vue3-course",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watch=true",
    "backend": "json-server --watch db.json --port 5000 --delay=1000 -m ./node_modules/json-server-auth",
    "serve:fullstack": "concurrently \"npm run backend\" \"npm run serve\""
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.6.1",
    "core-js": "^3.19.0",
    "jsonwebtoken": "^8.5.1",
    "vee-validate": "^4.5.4",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^13.5.0",
    "@testing-library/vue": "^6.4.0",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "3.2.20",
    "@vue/test-utils": "^2.0.0-rc.16",
    "concurrently": "^6.3.0",
    "json-server": "^0.17.0",
    "json-server-auth": "^2.1.0",
    "msw": "^0.35.0",
    "prettier": "^2.4.1",
    "vue-jest": "^5.0.0-alpha.10"
  }
